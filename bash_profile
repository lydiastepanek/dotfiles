[ -n "$PS1" ] && source ~/.ccenv;

# Load rbenv automatically by appending
# the following to ~/.bashrc:
eval "$(rbenv init -)"

export NVM_DIR="$HOME/.nvm"
. "$(brew --prefix nvm)/nvm.sh"

